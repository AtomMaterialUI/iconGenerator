{
  "name": "atom-icon-generator",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main",
  "bin": {
    "wiki": "./bin/wiki",
    "example": "./bin/example",
    "preview": "./bin/preview"
  },
  "scripts": {
    "postinstall": "npm run compile",
    "compile": "tsc",
    "compile:w": "npm run compile -- -w",
    "lint": "tslint --project tsconfig.json",
    "preexample": "npm run lint && npm run compile && npm run convert",
    "example": "ts-node ./bin/examples --all",
    "wiki": "ts-node ./bin/wiki all -o file",
    "wiki_publish": "ts-node ./bin/wiki all -o repo -t 58b3cf5064502644596f9cfdd76c007c6e960f53",
    "preview": "ts-node ./bin/preview all -o file",
    "convert": "npm run convert_icons && npm run convert_folders",
    "convert_icons": "xml2json icon_associations.xml icon_associations.json",
    "convert_folders": "xml2json folder_associations.xml folder_associations.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.7.6",
    "@types/yargs": "^15.0.4",
    "nodegit": "^0.26.5",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "xml2json-cli": "^1.1.1",
    "yargs": "^15.1.0"
  }
}
